{
  "name": "@vision/studio",
  "version": "0.1.34",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": ". .env && react-scripts start",
    "build": "react-scripts build",
    "build:develop": ". .env.development && react-scripts build",
    "dev:circular-deps": "npx madge --circular src/",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom --colors",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "tdd": "osascript ./scripts/tdd.scpt",
    "cypress": "$(npm bin)/cypress open",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sync-core": "yarn upgrade @vision/core",
    "chromatic": "npx chromatic --project-token=chpt_11b663df63e0638 --exit-zero-on-changes",
    "version:json": "node scripts/version.js",
    "changelog": "npx auto-changelog -p && git add CHANGELOG.md",
    "hotpush": "bash ./scripts/git-hotpush.sh",
    "git:hp": "yarn hotpush",
    "v:version": "node scripts/version.js",
    "v:push": "node ./scripts/git-hotpush.js"
  },
